    "@ganache/ethereum-address": "0.9.0",
    "@ganache/ethereum-block": "0.9.0",
    "@ganache/ethereum-options": "0.9.0",
    "@ganache/ethereum-transaction": "0.9.0",
    "@ganache/ethereum-utils": "0.9.0",
    "@ganache/promise-queue": "0.4.0",
    "@ganache/rlp": "0.9.0",
